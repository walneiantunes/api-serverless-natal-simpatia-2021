service: natal-simpatia-2021-service
frameworkVersion: "2"

provider:
  name: aws
  runtime: nodejs12.x
  lambdaHashingVersion: 20201221
  region: us-east-1
  memorySize: 512

functions:
  natal-2021:
    handler: handler.handler
    events:
      - http:
          path: category
          method: get
      - http:
          path: stores/{category}
          method: get
plugins:
  - serverless-offline
